<Application x:Class="MaintenancePlatform.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/interactivedesigner/2006"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:converter="clr-namespace:MaintenancePlatform.Converters"
             StartupUri="MainWindow.xaml"
             mc:Ignorable="d">

    <Application.Resources>

        <converter:ImageSourceConverter x:Key="isConverter" />

        <LinearGradientBrush x:Key="NormalBrush" EndPoint="0,1" StartPoint="0,0">
            <GradientStop Color="#EEE" Offset="0.0"/>
            <GradientStop Color="#CCC" Offset="1.0"/>
        </LinearGradientBrush>
        <LinearGradientBrush x:Key="NormalBorderBrush" EndPoint="0,1" StartPoint="0,0">
            <GradientStop Color="#CCC" Offset="0.0"/>
            <GradientStop Color="#444" Offset="1.0"/>
        </LinearGradientBrush>

        <!-- 图片按钮样式 -->
        <Style x:Key="ButtonPicShow" TargetType="{x:Type Button}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Grid  HorizontalAlignment="Left">
                            <Rectangle x:Name="rectangle" Height="24" Margin="0" Stroke="#00C1D8" >
                                <Rectangle.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#007590" Offset="1"/>
                                        <GradientStop Color="#007590"/>
                                        <GradientStop Color="#007590" Offset="0.506"/>
                                        <GradientStop Color="#007590" Offset="0.506"/>
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Width="105">
                                <Image Width="10" Source="\image\menu1.png" Margin="5" />
                                <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="图片轨迹" Foreground="White" FontSize="14"/>
                            </StackPanel>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsFocused" Value="True"/>
                            <Trigger Property="IsDefaulted" Value="True"/>
                            <MultiTrigger>
                                <MultiTrigger.Conditions>
                                    <Condition Property="IsMouseOver" Value="True" />
                                    <Condition Property="IsEnabled" Value="True" />
                                </MultiTrigger.Conditions>
                                <Setter Property="Fill" TargetName="rectangle">
                                    <Setter.Value>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFFFE1AA" Offset="1"/>
                                            <GradientStop Color="#FFFFEAC5"/>
                                            <GradientStop Color="#FFFFE8B8" Offset="0.506"/>
                                            <GradientStop Color="#FFFFD900" Offset="0.532"/>
                                        </LinearGradientBrush>
                                    </Setter.Value>
                                </Setter>
                                <Setter Property="Stroke" TargetName="rectangle" Value="#FF7B5529"/>
                            </MultiTrigger>
                            <Trigger Property="IsPressed" Value="True">
                                <Setter Property="Fill" TargetName="rectangle">
                                    <Setter.Value>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFFFE1AA" Offset="1"/>
                                            <GradientStop Color="#FFFFEAC5"/>
                                            <GradientStop Color="#FFFFDA8C" Offset="0.506"/>
                                            <GradientStop Color="#FFF1CD00" Offset="0.532"/>
                                        </LinearGradientBrush>
                                    </Setter.Value>
                                </Setter>
                                <Setter Property="Stroke" TargetName="rectangle" Value="#FF7B5529"/>
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="False"/>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="FontFamily" Value="Heiti"/>
            <Setter Property="FontSize" Value="14"/>
            <Setter Property="Foreground" Value="#FF034379"/>
        </Style>
        
